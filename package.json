{
  "name": "sl2m",
  "version": "0.0.1",
  "description": "This is a web application build using NodeJS which will take artist name as the input and display a list of albums using iTunes API",
  "main": "app.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmitThakkar/SL2M.git"
  },
  "keywords": [
    "Angular2",
    "NodeJS",
    "ExpressJS",
    "iTunes",
    "API"
  ],
  "author": "Amit Thakkar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AmitThakkar/SL2M/issues"
  },
  "homepage": "https://github.com/AmitThakkar/SL2M#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6",
    "express": "^4.14.0",
    "http-status": "^0.2.3",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
